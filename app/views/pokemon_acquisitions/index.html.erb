<%= render partial: "shared/header"%>

<p style="color: green"><%= notice %></p>

<div class="container">
  <h1 class="text-center">Pokemon acquisitions</h1>

  <div id="pokemon_acquisitions" class="container p-1">
    <% @pokemon_acquisitions.each do |pokemon_acquisition| %>
      <%= render pokemon_acquisition %>
      <p>
        <%= link_to "Show this pokemon acquisition", pokemon_acquisition %>
      </p>
    <% end %>

  <%= link_to "New pokemon acquisition", new_pokemon_acquisition_path %>

  </div>
</div>

